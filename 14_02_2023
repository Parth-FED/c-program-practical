[1].................................................................................................
//add,sub,mul,div,mod = all

#include<stdio.h>
int main()

{
	int a,b,add,sub,mul,div,mod;
		printf("\n enter the value of a and b: ");
		scanf("%d %d",&a,&b);
		
		add=a+b;
		sub=a-b;
		mul=a*b;
		div=a/b;
		mod=a%b;
		
		printf("add%d \nsub%d \nmul%d \ndiv%d \nmod%d:%d",add,sub,mul,div,mod);
		
}


