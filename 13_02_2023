[1].......................................................................................................
//sweping.

#include<stdio.h>

main()
{
	int x,y;
	printf("\n enter the value of x: ");
	scanf("%d",&x);
	printf("\n enter the value of y: ");
	scanf("%d",&y);
	
	x=x+y;
	y=x-y;
	x=x-y;
	
	printf("\n after swapping \nx:%d \ny:%d",x,y);
	
	return 0; 
	
}


[2].......................................................................................................
//swappin by c program

#include<stdio.h>

main()

{
	int a,b,c;
	printf("\n enter the value of a: ");
	scanf ("%d", &a);
	printf("\n enter the value of b: ");
	scanf ("%d", &b);
	c=a;
	a=b;
	b=c;
	printf("\n after swapping n\a:%dn\d:%d",a,b,c);
	
	return 0;
	
	
	
	
}
