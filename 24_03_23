// C program to add or concatenale three string

#include<stdio.h>

int main()
{
	char s1[10];
	char s2[10];
	char s3[10];
	
	printf("Enter the fitst string: ");
	scanf("%s",&s1);
	printf("\nEnter the second string:");
	scanf("%s",&s2);
	printf("\nEnter the third string:");
	scanf("%s",&s3);
	
	
	printf("%s,strcat(s1,s2,s3)");
	
	return 0;
}


