// else if   condition

#include<stdio.h>

int main()
{
	int n;
	printf("\n Enter the value: ");
	scanf("%d",&n);
	
	if(n>10)
	{
		printf("\n This is positive");
	}
	else if(n<10)
	{
		printf("\n this is negetive");
	}
}


