// function sum(+),sub(-)
#include<stdio.h>

void fun();
void fun()

{
	int a,b,mul;
	printf("enter the value of a and b: \n");
	scanf("%d%d",&a,&b);
	mul=a*b;
	printf("total  value of twe number:%d\n",mul);
}
void fun1()
{
	int a,b,div;
	printf("enter the value of a and b:\n");
	scanf("%d%d",&a,&b);
	div=a/b;
	printf("total value of two number:%d\n",div);
}
void main()
{
	fun();
	fun1();
	return 0;
}
